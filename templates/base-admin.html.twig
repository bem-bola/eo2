<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta content="width=device-width, initial-scale=1" name="viewport" />

        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}

        <link rel="icon" href="{{ asset('images/logo/logo_danielEO_bleu-03.png')}}" />


        {# TEMPLATES BOOTSTRAP #}

        <!-- Custom fonts for this template -->
        <link href="{{ asset('boots/vendor/fontawesome-free/css/all.min.css') }}" rel="stylesheet" type="text/css">
        <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

        <!-- Custom styles for this template -->
        <link href="{{ asset('boots/css/sb-admin-2.min.css') }}" rel="stylesheet">

        <!-- Custom styles for this page -->
        <link href="{{ asset('boots/vendor/datatables/dataTables.bootstrap4.min.css') }}" rel="stylesheet">

        {# fin template #}

        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-+0n0xVW2eSR5OomGNYDnhzAbDsOXxcvSN1TPprVMTNDbiYZCxYbOOl7+AMvyTG2x" crossorigin="anonymous">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">
        <link rel="stylesheet" href="{{ asset('css/style.css')}}">


        {# bundle #}
        <link rel="stylesheet" href="{{ asset('css/custom.css')}}" type="text/css">

        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    </head>
    {% set currentPath = app.request.requesturi %}
    {% set namePath = app.request.attributes.get('_route') %}
    <body class="{% if namePath == 'app_login' %} bg-gradient-primary {% endif %}">
    
        {% block body %}{% endblock %}
    </body>
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js" integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.min.js" integrity="sha384-Atwg2Pkwv9vp0ygtn1JAojH0nYbwNJLPhwyoVbhoPwBhjQPR5VtM2+xf0Uwh9KtT" crossorigin="anonymous"></script>
    <script src="{{ asset('js/app-admin.js') }}"></script>

    {# templates bootstrap #}

     <!-- Bootstrap core JavaScript-->
    <script src="{{ asset('boots/vendor/jquery/jquery.min.js')}}"></script>
    <script src="{{ asset('boots/vendor/bootstrap/js/bootstrap.bundle.min.js') }}"></script>

    <!-- Core plugin JavaScript-->
    <script src="{{ asset('boots/vendor/jquery-easing/jquery.easing.min.js') }}"></script>

    <!-- Custom scripts for all pages-->
    {# <script src="{{ asset('boots/js/sb-admin-2.min.js') }}"></script> #}

    <!-- Page level plugins -->
    <script src="{{ asset('boots/vendor/datatables/jquery.dataTables.min.js') }}"></script>
    <script src="{{ asset('boots/vendor/datatables/dataTables.bootstrap4.min.js') }}"></script>

    <!-- Page level custom scripts -->
    {# <script src="{{ asset('boots/js/demo/datatables-demo.js') }}"></script> #}
</html>
